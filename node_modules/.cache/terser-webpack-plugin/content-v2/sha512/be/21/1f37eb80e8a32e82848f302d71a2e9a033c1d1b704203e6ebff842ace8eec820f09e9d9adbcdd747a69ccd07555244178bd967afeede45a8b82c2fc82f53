);\n\n  \n    // Render the UI for your table\n    return (\n        <>\n            {/* from https://react-table.tanstack.com/docs/examples/pagination\n        Pagination can be built however you'd like. \n        This is just a very basic UI implementation:\n      */}\n\n            <div className={classes.Table}>\n                <table {...getTableProps()}>\n                    <thead className={classes.TableHeader}>\n                        {headerGroups.map((headerGroup: any) => (\n                            <tr {...headerGroup.getHeaderGroupProps()}>\n                                {headerGroup.headers.map((column: any) => (\n                                    <th style={column.style} {...column.getHeaderProps()} className={classes.TableHeader}>\n                                        {column.render('Header')}\n                                    </th>\n                                ))}\n                            </tr>\n                        ))}\n                    </thead>\n                    <tbody {...getTableBodyProps()}>\n                        {page.map((row: Row<object>, indx: number) => {\n                            prepareRow(row);\n                            return (\n                                <tr\n                                    {...row.getRowProps()}\n                                        className={classes.TableRow\n                                    }\n                                >\n                                    {row.cells.map((cell, indx) => {\n                                        return (\n                                            <td {...cell.getCellProps()} className={classes.TableCell}>\n                                                {cell.render('Cell')}\n                                            </td>\n                                        );\n                                    })}\n                                </tr>\n                            );\n                        })}\n         